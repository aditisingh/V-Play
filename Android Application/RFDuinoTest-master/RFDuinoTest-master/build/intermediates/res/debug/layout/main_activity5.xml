<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.lannbox.rfduinotest.MainActivity5">

    <TextView android:text="@string/hello_world" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView10" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Click to Begin!"
        android:id="@+id/begin"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <view
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        class="com.lannbox.rfduinotest.MainActivity5.BallBounce"
        android:id="@+id/view"
        android:layout_below="@+id/textView10"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="59dp" />

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/begin"
        android:layout_alignEnd="@+id/view"
        android:layout_marginTop="65dp"
        android:id="@+id/canvasLayout"></RelativeLayout>

</RelativeLayout>
